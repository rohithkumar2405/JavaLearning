
public class Hello {
	public static void main(String args[]) {
		Sample s1 = new Sample();
		Sample s2 = new Sample();
		
		s1.a = 20 ;
		s1.b = 100 ;
		s2.a = 50 ;
		s2.b = 200;
		
		s1.display1();
		s1.display2();
		s2.display3();
		s2.display4();
		
		System.out.println(" s1.a = " + s1.a + " s1.b = " + s1.b + " s2.a = " + s2.a + " s2.b = " + s2.b);
	}
}
